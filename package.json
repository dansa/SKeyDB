{
  "name": "skeydb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run --run --exclude src/pages/BuilderPage*.test.tsx",
    "test:integration": "vitest run --run src/pages/BuilderPage.awakeners-teams.test.tsx src/pages/BuilderPage.import-export.test.tsx src/pages/BuilderPage.wheels-covenants.test.tsx src/pages/BuilderPage.selection.test.tsx",
    "test:quick": "vitest run --run src/pages/CollectionPage.test.tsx src/pages/collection/OwnedWheelBoxExport.test.tsx src/components/ui/DupeLevelDisplay.test.tsx",
    "test:watch": "vitest",
    "verify": "npm run lint && npm run test -- --run && npm run build",
    "data:sync-awakener-ids": "node scripts/sync-awakener-ids.mjs",
    "data:sync-posse-indices": "node scripts/sync-posse-indices.mjs",
    "data:sync-wheel-data": "node scripts/sync-wheel-data.mjs",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "fuse.js": "^7.1.0",
    "html-to-image": "^1.11.13",
    "immer": "^11.1.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@tailwindcss/vite": "^4.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.1",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.2.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run verify"
  }
}
